<% include header %>
</head>
<body class=" grey lighten-2">
    <% include navBar %>
    <div class="container">
        <div class="row">
            <div class="col s8 offset-s2">
                <% if(frequencias.length === 0){ %>
                    <p>Não há frequencias neste teste.</p>
                <%}else{ %>
                    <ul class="collection with-header">
                    <% if(permission === 'D'){ %>
                        <li class="collection-header"><h4>Frequentadores do Teste</h4></li>
                        <%for(var chave in frequencias){  %>
                            <li class="collection-item"><%="Codigo do aluno:" + frequencias[chave].codigo + " Nome:" + frequencias[chave].nome %></li>
                        <%}
                        }
                    else if(permission === 'A'){ %>
                        <li class="collection-header"><h4>Testes Frequentados</h4></li>
                       <% for(var chave in frequencias){  %>
                            <li class="collection-item"><%=" UC: " + frequencias[chave].unidadeCurricular +" Data: "+ frequencias[chave].data.substring(0, 10) + " Sala:" + frequencias[chave].sala %> </li>
                        <%}
                    }%>
                    </ul>
                <%} %>
            </div>
        </div>
    </div>
</body>
</html>